+/*  */
+static int tonum(char *c)
+{
+  int val = atoi(c);
+  switch (c[strlen(c)-1]) {
+  case 'K': case 'k': val *= (1 << 10); break;
+  case 'M': case 'm': val *= (1 << 20); break;
+  case 'G': case 'g': val *= (1 << 30); break;
