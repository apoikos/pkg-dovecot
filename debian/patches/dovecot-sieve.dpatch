+static int sieve_keep(void *ac, 
+		      void *ic ATTR_UNUSED,
+		      void *sc, void *mc, const char **errmsg ATTR_UNUSED)
+{
+    sieve_keep_context_t *kc = (sieve_keep_context_t *) ac;
