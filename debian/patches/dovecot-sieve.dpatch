+static int sieve_execute_error_handler(const char *msg, 
+				       void *ic ATTR_UNUSED,
+				       void *sc ATTR_UNUSED,
+				       void *mc ATTR_UNUSED)
+{
+    i_info("sieve runtime error: %s", msg);
+    return SIEVE_OK;
+}
