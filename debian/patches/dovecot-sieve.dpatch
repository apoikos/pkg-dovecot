+static int sieve_parse_error_handler(int lineno, const char *msg, 
+				     void *ic ATTR_UNUSED,
+				     void *sc)
+{
+    script_data_t *sd = (script_data_t *) sc;
+
+    if (sd->errors == NULL)
+	    sd->errors = str_new(default_pool, 1024);
+
+    str_printfa(sd->errors, "line %d: %s\n", lineno, msg);
+    return SIEVE_OK;
+}
